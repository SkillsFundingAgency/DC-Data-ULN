<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>__Target_Database__</TargetDatabaseName>
    <DeployScriptFileName>ESFA.DC.Data.ULN.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=__SQL_SERVER__;Integrated Security=True;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=True</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockWhenDriftDetected>False</BlockWhenDriftDetected>
    <RegisterDataTierApplication>True</RegisterDataTierApplication>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="ULNROUserPwd">
      <Value>__ULN_RO_User__</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="ULNRWUserPwd">
      <Value>__ULN_RW_User__</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BUILD_BUILDNUMBER">
      <Value>__BUILD_BUILDNUMBER__</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="DEPLOYMENT_DATETIME">
      <Value>__RELEASE_DEPLOYMENT_STARTTIME__</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BUILD_BRANCHNAME">
      <Value>__BUILD_SOURCEBRANCHNAME__</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>